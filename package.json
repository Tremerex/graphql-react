{
  "name": "graphql-react",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:server build:client",
    "build:client": "cross-env NODE_ENV=production NODE_MODE=client webpack --config ./build/webpack.config.prod.babel.js",
    "build:server": "cross-env NODE_ENV=production NODE_MODE=server webpack --config ./build/webpack.config.server.babel.js",
    "clean": "npm-run-all --parallel clean:client clean:server",
    "clean:client": "rimraf ./public/assets",
    "clean:server": "rimraf ./public/server",
    "debug": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./public/server/index.js --inspect-brk ./public/server/index.js",
    "start": "cross-env NODE_ENV=production nodemon --watch ./public/erver --exec babel-node -- ./public/server/index.js",
    "start:prod": "npm-run-all clean build start"
  },
  "devDependencies": {
